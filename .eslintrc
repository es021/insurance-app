{
  "root": true,
  "env": {
    "node": true,
    "es6": true
  },
  "rules": {
    "no-console": 0,
    "no-debugger": 0
  },
  "parserOptions": {
    "parser": "babel-eslint",
    "ecmaVersion": 8,
    "sourceType": "module",
    "no-restricted-imports": [
      "error",
      {
        "paths": [
          "import1",
          "import2"
        ]
      }
    ]
  }
}

